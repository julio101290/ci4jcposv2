{
    "name": "julio101290/boilerplatelog",
    "description": "CodeIgniter4 Boilerplatelog librery for save log of changes in other modules",
    "keywords": [
        "codeigniter4",
        "mysql",
        "log",
        "boilerplate"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "juliocesar101920",
            "email": "juliocesar101290@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "codeigniter4/framework": "^4.1",
        "codeigniter4/translations": "^4.0",
        "julio101290/auth": "^1.0",
        "julio101290/boilerplate": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "fakerphp/faker": "^1.13"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "julio101290\\boilerplatelog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@composer dump-autoload"
        ],
        "test": "phpunit --colors=always -vvv"
    }
}
