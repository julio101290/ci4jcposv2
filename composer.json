{
    "name": "julio101290/ci4jcpox",
    "description": "Este repositorio representa un conjunto de módulos modulares y extensibles desarrollados por Julio101290, diseñados para cubrir funcionalidades esenciales en la gestión empresarial en México, incluyendo inventarios, facturación electrónica (CFDI 4.0), complemento de pago y gestión para Carta Porte.",
    "license": "MIT",
    "type": "project",
    "homepage": "https://codeigniter.com",
    "support": {
        "forum": "https://forum.codeigniter.com/",
        "source": "https://github.com/codeigniter4/CodeIgniter4",
        "slack": "https://codeigniterchat.slack.com"
    },
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.0",
        "hermawan/codeigniter4-datatables": "^0.8.0",
        "julio101290/boilerplatbackup": "^1.0",
        "julio101290/boilerplate": "^1.4",
        "julio101290/boilerplatebranchoffice": "^1.0",
        "julio101290/boilerplatecartaporte": "dev-main",
        "julio101290/boilerplatecashtonnage": "^1.0",
        "julio101290/boilerplatecfdi": "^1.0",
        "julio101290/boilerplatecfdielectronicseries": "^1.0",
        "julio101290/boilerplatecompanies": "^1.3",
        "julio101290/boilerplatecomplementopago": "^1.0",
        "julio101290/boilerplatecomprobanterd": "^1.0",
        "julio101290/boilerplatecustumers": "^1.0",
        "julio101290/boilerplatedrivers": "^1.0",
        "julio101290/boilerplateinventory": "*",
        "julio101290/boilerplatelocations": "^1.0",
        "julio101290/boilerplateproducts": "^1.0",
        "julio101290/boilerplatequotes": "^1.0",
        "julio101290/boilerplateremolques": "dev-main",
        "julio101290/boilerplatesells": "^1.0",
        "julio101290/boilerplatesettings": "^1.0",
        "julio101290/boilerplatestorages": "^1.0",
        "julio101290/boilerplatesuppliers": "^1.0",
        "julio101290/boilerplatetypesmovement": "^1.0",
        "julio101290/boilerplatevehicles": "^1.0",
        "phpcfdi/sat-catalogos": "^0.3.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^10.5.16"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "app/Config/",
            "Hermawan\\DataTables\\": "app/Overrides/DataTables/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
